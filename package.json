{
  "name": "graphiql-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx",
    "preview": "vite preview",
    "precommit": "lint-staged",
    "cy:open-e2e": "cypress open --e2e --browser chrome",
    "cy:open-unit": "cypress open --component --browser chrome",
    "cy:run-e2e": "cypress run --e2e",
    "cy:run-unit": "cypress run --component",
    "cy:e2e": "start-server-and-test dev http-get://localhost:5173 cy:open-e2e"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "firebase": "^9.21.0",
    "react": "^18.2.0",
    "react-day-and-night-toggle": "^1.0.18",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.11.0",
    "react-spinners": "^0.13.8",
    "sass": "^1.62.1",
    "styled-components": "^5.3.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@cypress/code-coverage": "^3.10.4",
    "@cypress/react": "^7.0.3",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-react": "^4.0.0",
    "add": "^2.0.6",
    "cypress": "^12.11.0",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-istanbul": "^4.0.1",
    "yarn": "^1.22.19"
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "node_modules/.bin/eslint --max-warnings=0",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
